    "authors": [
      "Fabian & Contributors"
    ],
    "keywords": [
      "iobroker",
      "iobroker-adapter",
      "owlet",
      "smart-socks",
      "baby-monitor",
      "health",
      "cloud",
      "nodejs",
      "python",
      "mit-license"
    ],
    "mode": "daemon",
    "platform": "Javascript/Node.js",
    "engineTypes": [
      "javascript"
    ],
    "enabled": true,
    "messagebox": false,
    "compact": true,
    "adminUI": {
      "config": "json"
    },
    "license": "MIT"
  },
  "native": {
    "email": "",
    "password": "",
    "region": "europe",
    "interval": 10,
    "pythonBin": "",
    "useVenv": false,
    "debugWorker": false,
    "implementation": "node",
    "override": {
      "apiKey": "",
      "url_base": "",
      "url_mini": "",
      "url_signin": "",
      "app_id": "",
      "app_secret": ""
    }
